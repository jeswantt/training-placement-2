class Solution(object):
    def twoSum(self, numbers, target):
        result = []
        left = 0
        right = len(numbers) - 1

        while left<right:
            if numbers[left]+numbers[right]>target:
                right = right-1
            elif numbers[left]+numbers[right]<target:
                left = left+1
            else:
                result.append(left+1)
                result.append(right+1)
                break

        return result
