class Solution {
    public int countPoints(String rings) {
        int n = rings.length(), count = 0;

        HashMap<Character,Set<Character>> dict1 = new HashMap<>();

        for (int i = 1; i < rings.length(); i += 2){
            if (!dict1.containsKey(rings.charAt(i))){
                dict1.put(rings.charAt(i),new HashSet<>());
                dict1.get(rings.charAt(i)).add(rings.charAt(i-1));
            } else {
                dict1.get(rings.charAt(i)).add(rings.charAt(i-1));
            }
        }

        for (char v: dict1.keySet()){
            if (dict1.get(v).size() == 3){
                count += 1;
            }
        }

        return count;
    }
}
