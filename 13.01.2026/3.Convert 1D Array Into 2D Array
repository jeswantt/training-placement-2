class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
        int l=original.length;
    
        int k=0;
        if(m*n==l)
        {
             int [][]arr=new int [m][n];
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
arr[i][j]=original[k];
k++;
    }
}

    


return arr;


        }   


            return new int[0][0];

}
}
