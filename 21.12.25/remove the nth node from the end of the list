class Solution(object):
    def removeNthFromEnd(self, head, n):
	
        slow = head 
        fast = head 
        for i in range(n): 
            fast = fast.next
        
        if not fast:
            return slow.next
        
        while fast.next:
            fast = fast.next
            slow = slow.next
            
        slow.next = slow.next.next
        return head
