class Solution(object):
    def swapPairs(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        dummy = ListNode(0)
        dummy.next = head
        prev = dummy
        ret = dummy

        while head and head.next:
            firstNode = head
            secondNode = head.next

            prev.next = secondNode
            nextTemp = secondNode.next
            secondNode.next = firstNode
            firstNode.next = nextTemp

            prev = firstNode

            head = firstNode.next


        return ret.next
