import collections 
class Solution(object):
    def isValidSudoku(self, board):
        row = defaultdict(list)
        col = defaultdict(list)
        cube = defaultdict(list)
        for r in range(9):
            for c in range(9):
                val = board[r][c]
                if val == ".":
                    continue
                if (val in row[r]) or (val in col[c]) or (val in cube[r//3,c//3]):
                    return False
                row[r].append(val)
                col[c].append(val)
                cube[(r//3),(c//3)].append(val)
        return True
   
