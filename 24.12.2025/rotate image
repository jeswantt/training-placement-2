class Solution(object):
    def rotate(self, matrix):
        
        for i in range(len(matrix)):
            for j in range(i + 1, len(matrix[0])):
                matrix[i][j], matrix[j][i] = matrix[j][i], matrix[i][j]
       
        for i in matrix:
            low, high = 0, len(i) - 1
            while low <= high:
                i[low], i[high] = i[high], i[low]
                low += 1
                high -= 1
