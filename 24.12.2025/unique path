class Solution(object):
    def uniquePaths(self, m, n):
        """
        :type m: int
        :type n: int
        :rtype: int
        """
        t = [[None for _ in range(n)] for _ in range(m)]

        for i in range(n):
            t[0][i] = 1
        for i in range(m):
            t[i][0] = 1

        i, j = m, n

        for i in range(1, m):
            for j in range(1, n):
                t[i][j] = t[i-1][j] + t[i][j-1]

        return t[m-1][n-1]
