class Solution(object):
    def subsetsWithDup(self, nums):
        nums.sort()   
        result, path = [], []
        n = len(nums)

        def dfs(start):
            result.append(path[:])  
            for i in range(start, n):
                if i > start and nums[i] == nums[i - 1]:
                    continue   
                path.append(nums[i])
                dfs(i + 1)
                path.pop()

        dfs(0)
        return result
