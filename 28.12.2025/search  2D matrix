class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
      for(int i = 0 ; i <matrix.length ; i++){
if(target(matrix[i],target)){
    return true;
}

      }
      return false;
    }
    public boolean target( int[] a ,int tar){
for(int i = 0 ; i<a.length;i++){
    if(a[i]==tar){
        return true;
    }
}

return false;
    }
}
