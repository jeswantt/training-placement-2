class Solution {
    public void sortColors(int[] nums) {
        int[] freq=new int[3];
        for(int i=0;i<nums.length;i++){
            freq[nums[i]]++;
        }
        int k=0;
        for(int i=0;i<nums.length;){
            if(freq[k] == 0){
                k++;
                continue;
            }
            nums[i++]=k;
            freq[k]--;
        }
    }
}
