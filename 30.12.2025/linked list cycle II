class Solution(object):
    def detectCycle(self, head):
        """
        :type head: ListNode
        :rtype: ListNode
        """
        slow=head
        fast=head
        while fast and fast.next:
            slow=slow.next
            fast=fast.next.next
            if slow == fast:
                slow=head
                while True:
                    if slow==fast:
                        return slow
                    slow=slow.next
                    fast=fast.next
        return None           
