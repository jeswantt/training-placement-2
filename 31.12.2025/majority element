class Solution {
    public int majorityElement(int[] a) {
        int vc=0;
        int can=0;
        for(int i=0;i<a.length;i++){
            int cn=a[i];
            if(vc==0){
                can=cn;
            }
            if(cn==can){
                vc++;
            }else{
                vc--;
        }
    }
        return can;
    }
}
